<style>
/*--------------------------------------------------------------------- import Fonts ---------------------------------------------------------------------*/
@import url('https://fonts.googleapis.com/css?family=Poppins:400,500,600,700');

/*--------------------------------------------------------------------- skeleton ---------------------------------------------------------------------*/
* {
     box-sizing: border-box;
     transition: ease all 0.5s;
}

body {
     color: #666666;
     font-size: 14px;
     font-family: 'Poppins', sans-serif;
     line-height: 1.8;
     overflow-x: hidden;
}

a {
     color: #1f1f1f;
     text-decoration: none !important;
     transition: all .3s ease-in-out;
}

h1, h2, h3, h4, h5, h6 {
     color: #111111;
     margin: 0;
}

.header_section {
       position: relative; /* Ajoutez cette ligne */
    z-index: 1; /* Ajoutez cette ligne */

	width: 100%;
	background-image: url(../images/banner-bg.png);
	height: auto;
	background-size: cover;
	background-repeat: no-repeat;
    padding: 25px 0;
}

.green-bold-logo {
    font-size: 24px;
    color: green;
    font-weight: bold;
}

.menu_main a {
    color: #ffffff;
}

.navbar-expand-lg .navbar-nav .nav-link {
   padding: 0 16px; 
   font-size: 20px;
   color: #ffffff;
} 

.nav-link:focus, .nav-link:hover {
    color: #000 !important;
}

.footer_section {
	width: 100%;
	background-color: #303330;
	height: auto;
	margin-top: 90px;
    padding-bottom: 90px;
}
.nav-link {
    pointer-events: auto; /* Assurez-vous que les événements de clic sont activés */
}

.copyright_text {
	width: 100%;
	text-align: center;
	font-size: 16px;
	color: #ffffff;
}
.copyright_text a {
	color: #ffffff;
}
.copyright_text a:hover {
	color: #000000;
}
</style>
{% block body %}
{% endblock %}